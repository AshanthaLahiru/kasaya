{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "jest"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "max-len": 0,
        "no-eval": [
            "error",
            {
                "allowIndirect": true
            }
        ],
        "no-template-curly-in-string": 0,
        "no-else-return": 0,
        "consistent-return": 0
    }
}